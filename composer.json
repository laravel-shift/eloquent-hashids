{
    "name": "mtvs/eloquent-hashids",
    "description": "On-the-fly hashids for Laravel Eloquent models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Ali Tavassoli",
            "email": "mtvs.dev@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^9.0",
        "vinkla/hashids": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5.10",
        "laravel/legacy-factories": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Mtvs\\EloquentHashids\\": "src/"
        }
    },
    "autoload-dev": {
